{{#sy-list model=model loading=true as |section|}}
  {{#if (eq section 'actions')}}
    {{yield section model}}
  {{else if (eq section 'header')}}
    {{yield section model}}
  {{else if (eq section 'footer')}}
    {{yield section model}}
  {{else if (eq section 'row')}}
    <tr><td colspan="42">&nbsp;</td></tr>
  {{/if}}
{{/sy-list}}
