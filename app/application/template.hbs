{{#sy-sidenav expand=expandSidenav}}
  <main class="page-main">
    {{outlet 'subnav'}}
    <div class="page-content page-content--scroll">
      {{#bread-crumbs class='breadcrumb--first-mobileonly' as |component crumb|}}
        {{crumb.title}}{{#if (equal crumb.title 'Loading')}}<span class="loading-dots"><i>.</i><i>.</i><i>.</i></span>{{/if}}
      {{/bread-crumbs}}
      {{outlet}}
    </div>
  </main>
{{/sy-sidenav}}

